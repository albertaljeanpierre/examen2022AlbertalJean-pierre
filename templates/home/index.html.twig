{% extends 'base.html.twig' %}

{% block title %}Liste des chansons {% endblock %}

{% block body %}


<div class="example-wrapper">
    <h1>Liste des chansons   </h1>
    <ul>
        {% for chanson in chansons %}
            <li><a href="{{ path('app_chanson',  {id: chanson.id }) }}">{{ chanson.titre }}</a></li>
        {% endfor %}
    </ul>

</div>
{% endblock %}
